    permission="zope2.View" />

  <browser:page
    name="viewFileGDoc"
    for="genweb6.organs.content.file.file.IFile"
    class=".firmes.ViewFile"
    layer="genweb6.organs.interfaces.IGenweb6OrgansLayer"
    permission="zope2.View" />

  <browser:page
    name="downloadFileGDoc"
    for="genweb6.organs.content.file.file.IFile"
    class=".firmes.DownloadFile"
    layer="genweb6.organs.interfaces.IGenweb6OrgansLayer"
    permission="zope2.View" />

