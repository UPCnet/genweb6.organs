  <browser:page
    name="downloadFile"
    for="genweb6.organs.content.acta.acta.IActa"
    class=".acta.DownloadFile"
    layer="genweb6.organs.interfaces.IGenweb6OrgansLayer"
    permission="zope2.View" />

